			  label="COM_CONFERENCEPLUS_GROUP_BASIC"
			  description="COM_CONFERENCEPLUS_GROUP_BASIC_DESC"
	>

		<field name="name" type="text" class="inputbox form-control"  labelclass="control-label"
		       size="150" label="COM_CONFERENCEPLUS_NAME"
		       readonly="true" />

		<field name="created" type="text" class="inputbox form-control"  labelclass="control-label"
		       size="150" label="COM_CONFERENCEPLUS_PAYMENT_CREATED"
		       readonly="true" />

		<field name="processkey" type="text" class="inputbox form-control"  labelclass="control-label"
		       size="150" label="COM_CONFERENCEPLUS_PROCESSKEY"
		       readonly="true" />

		<field name="processdata" type="textarea" class="inputbox form-control"  labelclass="control-label"
		       rows="50" cols="80" label="COM_CONFERENCEPLUS_PROCESSDATA"
		       readonly="true" />


	</fieldset>

